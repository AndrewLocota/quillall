INFO:root:Received request data: {'article': ' $(\'#article-input\').val(data.summary);  // Set the value of the article input form to the summary$(function() {\n    fetch(\'demographics.json\')\n        .then(response => response.json())\n        .then(demographics => {\n            $("#demographic-input").autocomplete({\n                source: demographics\n            });\n        })\n        .catch(error => console.error(\'Error:\', error));\n});\n\nvar clearAllButton = $(\'#clear-all\');\nvar demographicList = $(\'#demographic-list\');\nvar demographicsData; // Declare a variable to store the demographics data\n\nfetch(\'demographics.json\')\n.then(response => response.json())\n.then(demographics => {\n    $("#demographic-input").autocomplete({\n        source: demographics\n    });\n\n    setInterval(function() {\n        let randomIndex1, randomIndex2;\n        do {\n            randomIndex1 = Math.floor(Math.random() * demographics.length);\n            randomIndex2 = Math.floor(Math.random() * demographics.length);\n        } while (randomIndex1 === randomIndex2); // Ensure the two indices are different\n\n        let example1 = demographics[randomIndex1];\n        let example2 = demographics[randomIndex2];\n        $(\'#demographic-input\').attr(\'placeholder\', `e.g. ${example1}, ${example2}...`);\n    }, 4000);\n})\n.catch(error => console.error(\'Error:\', error));\n\n$(\'#submit-article\').click(function() {\n    var article = $(\'#article-input\').val();\n    var demographics = [];\n    $(\'#demographic-list li\').each(function() {\n        demographics.push($(this).text());\n    });\n\n    // Log the data you\'re sending\n    console.log(\'Sending data:\', { article: article, demographics: demographics });\n\n    // Add the frosted glass and pop out effects to the article form\n    $(\'#article-input\').addClass(\'frosted-glass pop-out\');\n    // Start loading animation\n    $(\'#article-input\').addClass(\'loading\');\n    $(\'.loader\').show();\n\n    fetch(\'/api/summarize\', {\n        method: \'POST\',\n        headers: {\n            \'Content-Type\': \'application/json\',\n        },\n        body: JSON.stringify({ article: article, demographics: demographics }),\n    })\n    .then(response => response.json())\n    .then(data => {\n        console.log(\'Response from server:\', data);\n        // Here you can handle the response from the server.\n        // For example, you could display the summarized text on the page.\n    })\n    .catch((error) => {\n        console.error(\'Error:\', error);\n    })\n\n    .finally(() => {\n        // Stop loading animation and remove frosted glass effect\n        $(\'#article-input\').removeClass(\'loading frosted-glass pop-out\');\n        $(\'.loader\').hide();\n    });\n});\n\n$(\'#add-demographic\').click(function() {\n    var input = $(\'#demographic-input\');\n    var demographics = input.val().split(\',\'); // Split the input by commas\n    input.val(\'\');\n\n    demographics.forEach(function(demographic) {\n        demographic = demographic.trim(); // Remove leading and trailing spaces\n\n        if (demographic) {\n            var li = $(\'<li>\').addClass(\'demographic-item\').text(demographic);\n\n            var remove = $(\'<span>\').text(\'X\').addClass(\'remove-demographic\').click(function() {\n                li.remove();\n                if (!demographicList.hasChildNodes()) {\n                    clearAllButton.removeClass(\'show\');\n                }\n            });\n\n            li.append(remove);\n            demographicList.append(li);\n            clearAllButton.addClass(\'show\');\n        }\n    });\n});\n\n$(\'#demographic-input\').keypress(function(e) {\n    if (e.which == 13) { // \'Enter\' key\n        e.preventDefault(); // Prevent the default form submission\n        $(\'#add-demographic\').click(); // Trigger the click event\n    }\n});\n\n$(\'#clear-all\').click(function() {\n    demographicList.empty();\n    clearAllButton.removeClass(\'show\');\n});\n\n$(\'#article-input\').on(\'input\', function() {\n    let inputLength = $(this).val().length;\n    let remaining = 3000 - inputLength;\n    if (remaining < 0) {\n        $(this).css(\'color\', \'red\');\n    } else {\n        $(this).css(\'color\', \'black\');\n    }\n    $(\'#counter\').text(`${inputLength}/3000`);\n});', 'demographics': []}
